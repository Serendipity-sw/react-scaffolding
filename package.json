    "url-loader": "^4.1.1",
    "webpack": "^5.76.3",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.0.1",
    "webpack-dashboard": "^3.3.7",
    "webpack-dev-server": "^4.13.1",
    "webpack-merge": "^5.8.0",
    "webpack-open-browser": "^2.0.2",
    "webpack-parallel-uglify-plugin": "^2.0.0",
    "webpackbar": "^5.0.0-3"
  },
  "dependencies": {
    "moment": "^2.29.4",
    "qs": "^6.11.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
