{
  "name": "react-scaffolding",
  "version": "1.0.0",
  "description": "scaffolding",
  "scripts": {
    "format:font-language": "prettier src/locale/language/**/* --write",
    "build-time": "node build-time.js",
    "rm-dist": "node remove-dist.js",
    "zip": "node build-zip.js",
    "analyze": "webpack --progress --mode production -c ./config/webpack/webpack.analyze.js",
    "start": "webpack serve --progress --mode development -c ./config/webpack/webpack.dev.js",
    "build": "pnpm run rm-dist && webpack --progress --mode production -c ./config/webpack/webpack.build.js && pnpm run build-time",
    "build:zip": "pnpm run build && pnpm run zip",
    "export-tran": "webpack --progress --mode production -c ./config/export-translation-webpack/webpack.js",
    "translation-js-convert-excel": "node ./service/translation-node-service/convert-js-to-excel.js",
    "export-excel": "pnpm run export-tran && pnpm run translation-js-convert-excel",
    "translation-excel-determine": "cd service/translation-service && translation-service.exe -d",
    "translation": "pnpm run export-excel && pnpm run translation-excel-determine && pnpm run format:font-language"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swgloomy/react-scaffolding.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/swgloomy/react-scaffolding/issues"
  },
  "homepage": "https://github.com/swgloomy/react-scaffolding#readme",
  "dependencies": {
    "@tanstack/query-async-storage-persister": "^5.85.5",
    "@tanstack/react-query": "^5.85.5",
    "@tanstack/react-query-persist-client": "^5.85.5",
    "dayjs": "^1.11.13",
    "i18next": "^25.4.0",
    "idb": "^8.0.3",
    "nanoid": "^5.1.5",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-i18next": "^15.7.1",
    "react-router": "^7.8.2",
    "react-router-dom": "^7.8.2",
    "rxjs": "^7.8.2",
    "throttle-debounce": "^5.0.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/plugin-transform-class-static-block": "^7.28.3",
    "@babel/plugin-transform-private-methods": "^7.27.1",
    "@babel/preset-react": "^7.27.1",
    "archiver": "^7.0.1",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^10.0.0",
    "compression-webpack-plugin": "^11.1.0",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "cssnano": "^7.1.1",
    "html-webpack-plugin": "^5.6.4",
    "mini-css-extract-plugin": "^2.9.4",
    "portfinder-sync": "^0.0.2",
    "postcss": "^8.5.6",
    "postcss-advanced-variables": "^5.0.0",
    "postcss-atroot": "^0.2.4",
    "postcss-import": "^16.1.1",
    "postcss-initial": "^3.0.4",
    "postcss-loader": "^8.1.1",
    "postcss-nested": "^7.0.2",
    "postcss-preset-env": "^10.3.0",
    "postcss-property-lookup": "^3.0.0",
    "postcss-use": "^3.0.0",
    "postcss-utilities": "^0.8.4",
    "prettier": "^3.6.2",
    "prettier-plugin-css-order": "^2.1.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.14",
    "thread-loader": "^4.0.4",
    "webpack": "^5.101.3",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1",
    "webpack-dashboard": "^3.3.8",
    "webpack-dev-server": "^5.2.2",
    "webpack-merge": "^6.0.1",
    "webpack-open-browser": "^2.0.2",
    "webpackbar": "^7.0.0",
    "xlsx": "^0.18.5"
  }
}
