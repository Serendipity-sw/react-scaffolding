{
  "name": "react-scaffolding",
  "version": "1.0.0",
  "description": "scaffolding",
  "scripts": {
    "zip": "gulp buildTime && gulp zip",
    "start": "webpack serve --progress --mode development -c ./config/webpack/webpack.dev.js",
    "analyze": "webpack --progress --mode production -c ./config/webpack/webpack.analyze.js",
    "build-develop-fix": "webpack --progress --mode production -c ./config/webpack/webpack.dev.js",
    "build-release": "webpack --progress --mode production -c ./config/webpack/webpack.release.js",
    "build": "webpack --progress --mode production -c ./config/webpack/webpack.product.js",
    "build-develop-fix:zip": "yarn run build-develop-fix && yarn run zip",
    "build-release:zip": "yarn run build-release && yarn run zip",
    "build:zip": "yarn run build && yarn run zip",
    "upload-build-develop-fix": "yarn run build-develop-fix && gulp buildTime && gulp uploadDevelop",
    "upload-build-release": "yarn run build-release && gulp buildTime && gulp uploadRelease"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swgloomy/react-scaffolding.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/swgloomy/react-scaffolding/issues"
  },
  "homepage": "https://github.com/swgloomy/react-scaffolding#readme",
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@babel/plugin-proposal-private-methods": "^7.14.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.12.13",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.2",
    "cache-loader": "^4.1.0",
    "compression-webpack-plugin": "^10.0.0",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "cssnano": "^5.1.15",
    "cssnano-preset-advanced": "^5.3.10",
    "file-loader": "^6.2.0",
    "gulp": "^4.0.2",
    "gulp-zip": "^5.1.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.5",
    "portfinder-sync": "^0.0.2",
    "postcss": "^8.4.21",
    "postcss-advanced-variables": "^3.0.1",
    "postcss-assets": "^6.0.0",
    "postcss-atroot": "^0.2.3",
    "postcss-import": "^15.1.0",
    "postcss-initial": "^3.0.4",
    "postcss-loader": "^7.1.0",
    "postcss-nested": "^6.0.1",
    "postcss-preset-env": "^8.0.1",
    "postcss-property-lookup": "^3.0.0",
    "postcss-pxtorem": "^6.0.0",
    "postcss-short": "^5.0.0",
    "postcss-use": "^3.0.0",
    "postcss-utilities": "^0.8.4",
    "style-loader": "^3.3.2",
