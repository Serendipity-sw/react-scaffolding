    "prettier-plugin-organize-imports": "^4.1.0",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.14",
    "thread-loader": "^4.0.4",
    "webpack": "^5.99.7",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1",
    "webpack-dashboard": "^3.3.8",
    "webpack-dev-server": "^5.2.1",
    "webpack-merge": "^6.0.1",
    "webpack-open-browser": "^2.0.2",
    "webpackbar": "^7.0.0"
