{
  "name": "react-scaffolding",
  "version": "1.0.0",
  "description": "scaffolding",
  "scripts": {
    "build-time": "node build-time.js",
    "rm-dist": "node remove-dist.js",
    "zip": "node build-zip.js",
    "analyze": "webpack --progress --mode production -c ./config/webpack/webpack.analyze.js",
    "start": "webpack serve --progress --mode development -c ./config/webpack/webpack.dev.js",
    "build": "pnpm run rm-dist && webpack --progress --mode production -c ./config/webpack/webpack.build.js && pnpm run build-time",
    "build:zip": "pnpm run build && pnpm run zip"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swgloomy/react-scaffolding.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/swgloomy/react-scaffolding/issues"
  },
  "homepage": "https://github.com/swgloomy/react-scaffolding#readme",
  "dependencies": {
    "dayjs": "^1.11.13",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.5.3",
    "react-router-dom": "^7.5.3",
    "rxjs": "^7.8.2",
    "throttle-debounce": "^5.0.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.27.1",
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/plugin-transform-class-static-block": "^7.27.1",
    "@babel/plugin-transform-private-methods": "^7.27.1",
    "@babel/plugin-transform-runtime": "^7.27.1",
    "@babel/preset-react": "^7.27.1",
    "archiver": "^7.0.1",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^10.0.0",
    "compression-webpack-plugin": "^11.1.0",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "cssnano": "^7.0.6",
    "html-webpack-plugin": "^5.6.3",
    "mini-css-extract-plugin": "^2.9.2",
    "portfinder-sync": "^0.0.2",
    "postcss": "^8.5.3",
    "postcss-advanced-variables": "^5.0.0",
    "postcss-atroot": "^0.2.4",
    "postcss-import": "^16.1.0",
    "postcss-initial": "^3.0.4",
